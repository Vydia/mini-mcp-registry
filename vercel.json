{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "rewrites": [
    { "source": "/v0.1/servers", "destination": "/v0.1/servers.json" },
    { "source": "/v0.1/:name/versions/latest", "destination": "/v0.1/:name/versions/latest.json" },
    { "source": "/v0.1/:org/:name/versions/latest", "destination": "/v0.1/:org/:name/versions/latest.json" }
  ],
  "headers": [
    {
      "source": "/(.*\\.json)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Access-Control-Allow-Methods", "value": "GET, OPTIONS" },
        { "key": "Content-Type", "value": "application/json" }
      ]
    }
  ]
}
